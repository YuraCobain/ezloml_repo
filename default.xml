<?xml version="1.0" encoding="UTF-8"?>
<manifest>
  <default remote="gumstix" revision="default"/>

  <!-- You can have as many remote repositories as you like -->
  <remote name="yocto" fetch="git://git.yoctoproject.org" />
  <remote name="oe" fetch="git://git.openembedded.org" />
  <remote name="golang" fetch="git://github.com/madisongh" />
  <remote name="ezloml" fetch="git://github.com/YuraCobain" />
  <remote name="sunxi" fetch="git://github.com/linux-sunxi" />
  <!-- List all the repositories you want to fetch -->
  <!-- "name": git repository's name on hosting server
       "remote": the remote host from above (if not the default)
       "revision": commitish to checkout (if not the default)
       "path": directory where git repository gets cloned/synced -->

  <project name="meta-ezlo" revision="refs/heads/thud"  remote="ezloml" path="meta-sources" />
  <project name="meta-sunxi" revision="refs/heads/thud"  remote="sunxi" path="meta-sources" />
  <project name="meta-golang" revision="master"  remote="golang" path="meta-sources" />
  <project name="meta-openembedded" revision="refs/heads/thud"  remote="oe" path="meta-sources" />
  <project name="poky" revision="refs/heads/thud" remote="yocto" path="meta-sources" />

</manifest>

