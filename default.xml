<?xml version="1.0" encoding="UTF-8"?>
<manifest>
    <default remote="ezloteam" revision="default"/>

  <!-- You can have as many remote repositories as you like -->
  <remote name="ezloteam" fetch="git://github.com/ezloteam" />
  <remote name="yocto" fetch="git://git.yoctoproject.org" />
  <remote name="oe" fetch="git://git.openembedded.org" />
  <remote name="ezloml" fetch="git://github.com/ezloteam" />
  <remote name="sunxi" fetch="git://github.com/linux-sunxi" />
  <!-- List all the repositories you want to fetch -->
  <!-- "name": git repository's name on hosting server
       "remote": the remote host from above (if not the default)
       "revision": commitish to checkout (if not the default)
       "path": directory where git repository gets cloned/synced -->

  <project name="meta-ezlo" revision="sumo"  remote="ezloteam" path="meta-sources/meta-ezlo" />
  <project name="meta-sunxi" revision="sumo"  remote="sunxi" path="meta-sources/meta-sunxi" />
  <project name="meta-openembedded" revision="sumo"  remote="oe" path="meta-sources/meta-openembedded" />
  <project name="poky" revision="sumo" remote="yocto" path="meta-sources/poky" />

</manifest>

